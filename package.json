{
	"name": "dcmhs-portal",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development node server/index.js",
		"build": "vite build && vite build --ssr src/entry-server.jsx",
		"build:static": "vite build",
		"start": "NODE_ENV=production node server/index.js",
		"clean": "rm -rf dist node_modules/.cache",
		"lint": "biome lint .",
		"format": "biome format . --write",
		"predeploy": "GITHUB_PAGES=true npm run build:static",
		"deploy": "gh-pages -d dist/client",
		"vercel-build": "npm run build"
	},
	"dependencies": {
		"@emotion/react": "^11.13.0",
		"@emotion/server": "^11.11.0",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^7.3.2",
		"@mui/material": "^7.3.2",
		"@mui/utils": "^7.3.2",
		"@mui/x-date-pickers": "^8.19.0",
		"axios": "^1.13.2",
		"bcryptjs": "^3.0.2",
		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"express": "^5.0.0",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"jwt-decode": "^4.0.0",
		"mobx": "^6.13.7",
		"mobx-react": "^9.2.0",
		"mongoose": "^8.18.1",
		"morgan": "^1.10.0",
		"pino": "^9.3.2",
		"pino-http": "^10.3.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^6.30.1",
		"react-social-media-embed": "^2.5.18"
	},
	"optionalDependencies": {
		"pino-pretty": "^11.2.2"
	},
	"overrides": {},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@vitejs/plugin-react": "^5.0.0",
		"gh-pages": "^6.3.0",
		"vite": "^7.1.5"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
